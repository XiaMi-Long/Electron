    "lodash": "^4.17.21",
    "node-schedule": "^2.1.1",
    "pinia": "^2.1.4",
    "sass": "^1.63.6",
    "vue-router": "4",
    "wallpaper": "5.0.1",
    "xijs": "^1.2.6"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
